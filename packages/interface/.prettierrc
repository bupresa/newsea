{
  "useTabs": false,
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "jsxBracketSameLine": false,
  "semi": true,
  "quoteProps": "consistent",
  "importOrder": [
    "^@sd/core/(.*)$",
    "^@sd/interface/(.*)$",
    "^@sd/client/(.*)$",
    "^@sd/ui/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
